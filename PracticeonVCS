pipeline {Test for VCS)
  Tester: Arshad for VCS Version1.0
    stage('Build') {
      steps {
        echo 'Building...'
        sh './gradlew build'
      }
    }
    stage('Test') {
      steps {
        echo 'Testing...'
        sh './gradlew test'
      }
    }
    stage('Deploy') {
      steps {
        echo 'Deploying...'
        sh './gradlew deploy'
      }
    }
  }
}
